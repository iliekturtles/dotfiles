[
    {
        "data": [
            {{#each kde.monitors}}
            {
                "allowDdcCi": true,
                "allowSdrSoftwareBrightness": false,
                "autoRotation": "InTabletMode",
                "brightness": {{this.brightness}},
                "colorPowerTradeoff": "PreferEfficiency",
                "colorProfileSource": "sRGB",
                "connectorName": "{{this.connectorName}}",
                "detectedDdcCi": {{this.detectedDdcCi}},
                "edidHash": "{{this.edidHash}}",
                "edidIdentifier": "{{this.edidIdentifier}}",
                "edrPolicy": "always",
                "highDynamicRange": false,
                "iccProfilePath": "",
                "maxBitsPerColor": 0,
                "mode": {
                    "height": {{this.height}},
                    "refreshRate": {{this.refreshRate}},
                    "width": {{this.width}}
                },
                "overscan": 0,
                "rgbRange": "Automatic",
                "scale": {{this.scale}},
                "sdrBrightness": 200,
                "sdrGamutWideness": 0,
                "transform": "Normal",
                "uuid": "{{this.uuid}}",
                "vrrPolicy": "{{this.vrrPolicy}}",
                "wideColorGamut": false
            }{{#if (not @last)}},{{/if}}
            {{/each}}
        ],
        "name": "outputs"
    },
    {
        "data": [
            {
                "lidClosed": false,
                "outputs": [
                    {{#each kde.monitors}}
                    {
                        "enabled": true,
                        "outputIndex": {{@key}},
                        "position": {
                            "x": {{this.x}},
                            "y": {{this.y}}
                        },
                        "priority": 0,
                        "replicationSource": ""
                    }{{#if (not @last)}},{{/if}}
                    {{/each}}
                ]
            },
            {
                "lidClosed": true,
                "outputs": [
                    {
                        "enabled": true,
                        "outputIndex": 0,
                        "position": {
                            "x": 0,
                            "y": 0
                        },
                        "priority": 0,
                        "replicationSource": ""
                    }
                ]
            }
        ],
        "name": "setups"
    }
]
